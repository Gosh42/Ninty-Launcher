    if(def_textmod == 1){
        
        show_textx = def_posx-64;
        show_texty = def_posy;
    };
    //Cima
    else if(def_textmod == 2){
    
        show_textx = def_posx;
        show_texty = def_posy+64;
    };
    //Baixo
    else if(def_textmod == 3){
    
        show_textx = def_posx;
        show_texty = def_posy-64;
    };
    //Esquerda
    else{
    
        show_textx = def_posx+64;
        show_texty = def_posy;
    };
    
    //Próxima animação
    start = 1;
    alarm[0] = 10;
    alarm[1] = -1;
};
