            <string>///Baixar...

//1. Baixar  "dynamic.ini"
if(ds_map_find_value(async_load,"id") == async_redirect){

    //Se download finalizar (0) ou falhar (-1)...
    if(ds_map_find_value(async_load,"status") &lt; 1){
    
        //Finalizado
        downloading = 0;
        
        //Ler dados
        ini_open(game_save_id+"network\dynamic.ini");
        global.redirect_appdb = ini_read_string("REDIRECT","appdb","hpps://");
        global.redirect_updatelog = ini_read_string("REDIRECT","updatelog","hpps://");
        global.redirect_about = ini_read_string("REDIRECT","about","hpps://");
        ini_close();
        
        //Buscar por "appdb.ini" e "updatelog.ini"
        if(os_is_network_connected())
            event_user(1);
    }
    //Ainda baixando...
    else
        downloading = 1;
};

//2. Baixar "appdb.ini" e "updatelog.ini"
if(ds_map_find_value(async_load,"id") == async_appdb){

    //Se download finalizar...
    if(ds_map_find_value(async_load,"status") &lt; 1)
        downloading = 0;
    else
        downloading = 1;
};
if(ds_map_find_value(async_load,"id") == async_updatelog){

    //Se download finalizar...
    if(ds_map_find_value(async_load,"status") &lt; 1){
    
        //Finalizado
        downloading = 0;
        
        //Preparar pop-up no launcher
        with(obj_launcher)
            alarm[5] = 15;
    }
    else
        downloading = 1;
};
</string>
