            downloading = 1;
            async_appdb = http_get_file(global.redirect_appdb,game_save_id+"network\appdb.ini");
        }
        else{
        
            //Preparar pop-up no launcher
            with(obj_launcher){
                if(updateninty == 1)
                    alarm[5] = 15;
            }
        }
    }
    //Ainda baixando...
    else
        downloading = 1;
};

