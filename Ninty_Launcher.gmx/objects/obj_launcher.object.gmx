&amp;&amp;(dialog == 0)
&amp;&amp;(forwarder == 0)
&amp;&amp;(move == 0){
        
    //Pasta do Launcher
    if(point_in_rectangle(mouse_x,mouse_y,595,662,595+44,662+44))
