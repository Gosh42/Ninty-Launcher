            <string>//SE for permitido buscar níveis...
if(forwarder_find == 1){

    //SE não encontrar nenhum arquivo...
    if(forwarder_file == ""){  
        
        //Definir como nenhum arquivo
        forwarder_file = "???";
        
        //Terminar busca
        file_find_close();
        
        //Parar permissão de busca
        forwarder_find = 0;
    };
    
    //Enquanto existir algum arquivo durante busca...
    while(forwarder_file != ""){
         
         //Buscar pelo próximo
         forwarder_file = file_find_next();
         
         //Incluir NOME do arquivo a lista
         ds_map_add(forwarder_list,string(forwarder_total)+"_t",forwarder_file);

         //Incluir CAMINHO do arquivo a lista
         ds_map_add(forwarder_list,string(forwarder_total)+"_p",forwarder_path+forwarder_file);
                  
         //Incrementar total de aquivos
         forwarder_total++;
         
         //Se não tiver nenhum arquivo...
         if(forwarder_file == ""){
         
            //Parar loop
            break;
            exit;
         };
    };
};
</string>
